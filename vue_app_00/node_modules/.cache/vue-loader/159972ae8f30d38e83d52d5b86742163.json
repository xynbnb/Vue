-->\r\n    <!-- 轮播图 mint-ui-->\r\n    <mt-swipe>\r\n      <mt-swipe-item v-for=\"item of navlist\" :key=\"item.id\">\r\n        <img :src=\"item.img_url\" alt=\"\">\r\n      </mt-swipe-item>\r\n    </mt-swipe>\r\n    <!-- 九宫格 mui-->\r\n    <ul class=\"mui-table-view mui-grid-view mui-grid-9\">\r\n      <li v-for=\"item of gridlist\" class=\"mui-table-view-cell mui-media mui-col-xs-4 mui-col-sm-3\" :key=\"item.id\"><a  href=\"#\"  >\r\n        <img :src=\"item.img_url\" @click.prevent=\"jump\" :data-id=\"item.id\" >\r\n        <div class=\"mui-media-body\">{{item.title}}</div></a>\r\n      </li>\r\n    </ul>\r\n    <!-- tabbar.html  55~72  mui-->\r\n    \r\n  </div>\r\n</template>\r\n<script>\r\nexport default {\r\n  data(){\r\n    return{\r\n      navlist:[],    //轮播图列表 \r\n      gridlist:[]   //保存九宫格数组\r\n      }\r\n  },\r\n  created() {\r\n    this.handleImage();\r\n    this.handleGrid();\r\n  },\r\n  methods: {\r\n      jump(e){\r\n      var id = e.target.dataset.id;\r\n      var path = \"/\";\r\n      if(id==1){\r\n        path = \"/NewsList\"\r\n      }else if(id == 2){\r\n        path = \"/GoodList\"\r\n      }else{\r\n        path = \"/\"\r\n      }\r\n      //console.log(id);\r\n      //console.log(123);\r\n      this.$router.push(path);\r\n    },\r\n    handleGrid(){\r\n      //1.发送ajax获取九宫格数据\r\n      var url = \"http://127.0.0.1:3000/grid\";\r\n      this.axios.get(url).then(result=>{\r\n        //2.获取返回数据\r\n        console.log(result.data);\r\n      //3.保存gridlist\r\n      this.gridlist = result.data;\r\n      });\r\n    },\r\n    //加载轮播图的数据\r\n    handleImage(){\r\n      //1.发送ajax获取轮播数据\r\n      var url = \"http://127.0.0.1:3000/imglist\";\r\n      this.axios.get(url).then(result=>{\r\n        //2.获取返回结果\r\n      //console.log(result.data.data);\r\n      //3.保存data属性中\r\n      this.navlist=result.data.data;\r\n      })\r\n    }  \r\n  },\r\n}\r\n</script>\r\n<style scoped>\r\n.app-home .mint-swipe{\r\n  height:200px;\r\n}\r\n.app-home .mint-swipe img{\r\n  width:100%;\r\n}\r\n/* 九宫格图片的宽度 */\r\n.app-home .mui-grid-9 img{\r\n  width:60px;\r\n  height: 60px;\r\n}\r\n/* 九宫格图片的背景色 */\r\n.app-home .mui-grid-view.mui-grid-9{\r\n  background:#fff;\r\n}\r\n</style>"]}]}
t-header title=\"学子商城\" fixed></mt-header>\r\n    <!-- 轮播图 mint-ui-->\r\n    <mt-swipe>\r\n      <mt-swipe-item v-for=\"item of navlist\" :key=\"item.id\">\r\n        <img :src=\"item.img_url\" alt=\"\">\r\n      </mt-swipe-item>\r\n    </mt-swipe>\r\n    <!-- 九宫格 mui-->\r\n    <ul class=\"mui-table-view mui-grid-view mui-grid-9\">\r\n      <li v-for=\"item of gridlist\" class=\"mui-table-view-cell mui-media mui-col-xs-4 mui-col-sm-3\" :key=\"item.id\"><a href=\"#\">\r\n        <img :src=\"item.img_url\">\r\n        <div class=\"mui-media-body\">{{item.title}}</div></a>\r\n      </li>\r\n    </ul>\r\n    <!-- tabbar.html  55~72  mui-->\r\n    <nav class=\"mui-bar mui-bar-tab\">\r\n\t\t\t<a class=\"mui-tab-item mui-active\" href=\"#tabbar\">\r\n\t\t\t\t<span class=\"mui-icon mui-icon-home\"></span>\r\n\t\t\t\t<span class=\"mui-tab-label\">首页</span>\r\n\t\t\t</a>\r\n\t\t\t<a class=\"mui-tab-item\" href=\"#tabbar-with-chat\">\r\n\t\t\t\t<span class=\"mui-icon mui-icon-contact\"></span>\r\n\t\t\t\t<span class=\"mui-tab-label\">会员</span>\r\n\t\t\t</a>\r\n\t\t\t<a class=\"mui-tab-item\" href=\"#tabbar-with-contact\">\r\n\t\t\t\t<span class=\"mui-icon mui-icon-extra mui-icon-extra-cart\"><span class=\"mui-badge\">0</span></span>\r\n\t\t\t\t<span class=\"mui-tab-label\">购物车</span>\r\n\t\t\t</a>\r\n\t\t\t<a class=\"mui-tab-item\" href=\"#tabbar-with-map\">\r\n\t\t\t\t<span class=\"mui-icon mui-icon-search\"></span>\r\n\t\t\t\t<span class=\"mui-tab-label\">搜素</span>\r\n\t\t\t</a>\r\n\t\t</nav>\r\n  </div>\r\n</template>\r\n<script>\r\nexport default {\r\n  data(){\r\n    return{\r\n      navlist:[],    //轮播图列表 \r\n      gridlist:[]   //保存九宫格数组\r\n      }\r\n  },\r\n  created() {\r\n    this.handleImage();\r\n    this.handleGrid();\r\n  },\r\n  methods: {\r\n    handleGrid(){\r\n      //1.发送ajax获取九宫格数据\r\n      var url = \"http://127.0.0.1:3000/grid\";\r\n      this.axios.get(url).then(result=>{\r\n        //2.获取返回数据\r\n        console.log(result.data);\r\n      //3.保存gridlist\r\n      this.gridlist = result.data;\r\n      });\r\n    },\r\n    //加载轮播图的数据\r\n    handleImage(){\r\n      //1.发送ajax获取轮播数据\r\n      var url = \"http://127.0.0.1:3000/imglist\";\r\n      this.axios.get(url).then(result=>{\r\n        //2.获取返回结果\r\n      //console.log(result.data.data);\r\n      //3.保存data属性中\r\n      this.navlist=result.data.data;\r\n      })\r\n    }  \r\n  },\r\n}\r\n</script>\r\n<style scoped>\r\n.app-home .mint-swipe{\r\n  height:200px;\r\n}\r\n.app-home .mint-swipe img{\r\n  width:100%;\r\n}\r\n/* 九宫格图片的宽度 */\r\n.app-home .mui-grid-9 img{\r\n  width:60px;\r\n  height: 60px;\r\n}\r\n/* 九宫格图片的背景色 */\r\n.app-home .mui-grid-view.mui-grid-9{\r\n  background:#fff;\r\n}\r\n</style>"]}]}
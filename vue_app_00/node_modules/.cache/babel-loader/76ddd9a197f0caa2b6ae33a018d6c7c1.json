set.id;\r\n      //console.log(id);\r\n      //console.log(123);\r\n      //this.$router.push(\"/Goodlist\");\r\n    },\r\n    handleGrid(){\r\n      //1.发送ajax获取九宫格数据\r\n      var url = \"http://127.0.0.1:3000/grid\";\r\n      this.axios.get(url).then(result=>{\r\n        //2.获取返回数据\r\n        console.log(result.data);\r\n      //3.保存gridlist\r\n      this.gridlist = result.data;\r\n      });\r\n    },\r\n    //加载轮播图的数据\r\n    handleImage(){\r\n      //1.发送ajax获取轮播数据\r\n      var url = \"http://127.0.0.1:3000/imglist\";\r\n      this.axios.get(url).then(result=>{\r\n        //2.获取返回结果\r\n      //console.log(result.data.data);\r\n      //3.保存data属性中\r\n      this.navlist=result.data.data;\r\n      })\r\n    }  \r\n  },\r\n}\r\n</script>\r\n<style scoped>\r\n.app-home .mint-swipe{\r\n  height:200px;\r\n}\r\n.app-home .mint-swipe img{\r\n  width:100%;\r\n}\r\n/* 九宫格图片的宽度 */\r\n.app-home .mui-grid-9 img{\r\n  width:60px;\r\n  height: 60px;\r\n}\r\n/* 九宫格图片的背景色 */\r\n.app-home .mui-grid-view.mui-grid-9{\r\n  background:#fff;\r\n}\r\n</style>"],"sourceRoot":"src\\components\\tabbar"}]}